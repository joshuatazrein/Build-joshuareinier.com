    "@types/matter-js": "^0.18.2",
    "@types/p5": "^1.6.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "gsap": "^3.11.5",
    "isbot": "^3.6.8",
    "jquery": "^3.6.4",
    "lodash": "^4.17.21",
    "matter-attractors": "^0.1.6",
    "matter-js": "^0.19.0",
